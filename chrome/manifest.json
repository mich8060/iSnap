{
	"name": "Snap.me",
	"description": "The fastest and easiest way to visually bookmark your favorites while surfing the Web.",
	"version": "0.1",
  	"manifest_version": 2,
  	"browser_action": {
    	"default_icon": "icon.png",
    	"default_popup": "popup.html"
  	},
  	"permissions": [
    	"tabs",
    	"http://*/*",
    	"https://*/*",
    	"<all_urls>",
    	"notifications"
  	],
  	"icons": {
    	"16": "icon16.png",
    	"48": "icon48.png",
    	"128": "icon128.png"
  	}
}
